{
  "name": "horizon-preorder-feature",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:headed": "HEADLESS=false jest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:ci": "HEADLESS=true jest --ci",
    "prepare": "husky",
    "dev": "shopify theme dev --store=${SHOPIFY_STORE_URL}",
    "dev:hot": "shopify theme dev --store=${SHOPIFY_STORE_URL} --live-reload hot",
    "pull": "shopify theme pull --store=${SHOPIFY_STORE_URL}",
    "push": "shopify theme push --store=${SHOPIFY_STORE_URL} --theme=${SHOPIFY_THEME_ID}",
    "preview": "shopify theme open --store=${SHOPIFY_STORE_URL}",
    "lint:theme": "theme-check",
    "lint": "npm run lint:theme",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcoursen/dothan-preorder-theme.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dcoursen/dothan-preorder-theme/issues"
  },
  "homepage": "https://github.com/dcoursen/dothan-preorder-theme#readme",
  "description": "",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv": "^17.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^24.16.0",
    "puppeteer-core": "^24.16.0"
  }
}
